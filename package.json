{
  "name": "remix-rust",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "workspaces": [
    "config/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npx --yes turbo run build",
    "dev": "npx --yes turbo run dev --parallel",
    "lint": "npx --yes turbo run lint",
    "typecheck": "npx --yes turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md}\""
  },
  "devDependencies": {
    "patch-package": "^6.5.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.5.5",
  "dependencies": {

  }
}
